  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link rel='stylesheet' href='/stylesheets/edit.css' />
  <link href="http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.9/summernote.css" rel="stylesheet">
  <script src="http://cdnjs.cloudflare.com/ajax/libs/summernote/0.8.9/summernote.js"></script>
</head>
<body>
  <div class="fluid">

    <div class="row">
      <nav class="navbar navbar-inverse">
        <div class="container-fluid">
          <div class="navbar-header">
            <a class="navbar-brand" href="#">WomenInCoding</a>
          </div>
          <ul class="nav navbar-nav">
            <li><a href="/">Home</a></li>
            <li><a href="/new">New Post</a></li>
            <li><a href="/contact">Contact</a></li>
          </ul>
        </div>
      </nav>
    </div>
  </div>

  <div class="row">
    <div class="header">
      <h1>Edit a Post</h1>
    </div>
  </div>

</nav>
<form method="POST" action="/new" class="form">
  <div class="form-group">
    <label for="Title">Title</label>
    <input type="text" class="form-control" placeholder="Title"
    id="title" name="title">
  </div>
  <div class="form-group">
    <label for="Author">Author</label>
    <input type="text" class="form-control"
    placeholder="Author" id="Author" name="Author">
  </div>

  <div class="form-group">
    <label for="datetime">Date</label>
    <input type="date" class="form-control"
    placeholder="date" id="date" name="date">

  </div>
  <div class="form-group">
    <label for="datetime">Time</label>
    <input type="time" class="form-control"
    placeholder="time" id="time" name="time">
  </div>
  <div class="form-group">
    <label for="image">Image url</label>
    <input type="text" class="form-control"
    placeholder="image" id="image" name="image">
  </div>




  <textarea id="summernote" name="content"></textarea>

  <button type="submit" class="btn btn-primary">Post</button>
</form>
</div>
</div>



<script type="text/javascript">
$(document).ready(function() {
  $('#summernote').summernote();
});

</script>

